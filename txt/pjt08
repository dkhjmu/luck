<pipelines group="PJT08_RLS" pjtCode="11">
    <pipeline name="PJT08_RLS_package">
      <environmentvariables>
        <variable name="DEST">
          <value>C:\temp\11\rls</value>
        </variable>
      </environmentvariables>
      <materials>
        <pipeline pipelineName="PJT08_RLS_build" stageName="build" materialName="packaging" />
      </materials>
      <stage name="package">
        <jobs>
          <job name="package">
            <tasks>
              <exec command="java" args="-version">
                <runif status="passed" />
              </exec>
            </tasks>
          </job>
        </jobs>
      </stage>
    </pipeline>
    <pipeline name="PJT08_RLS_build">
      <materials>
        <git url="http://alm:platform0!@70.121.224.25/gitlab/alm/testpjt08.git" />
      </materials>
      <stage name="build">
        <approval type="manual" />
        <jobs>
          <job name="build">
            <tasks>
              <exec command="mvn" args="package">
                <runif status="passed" />
              </exec>
              <exec command="uploadFile">
                <arg>http://70.121.224.24:9081/scout/buildfile</arg>
                <arg>%DEST%</arg>
                <arg>%cd%/target</arg>
                <runif status="passed" />
              </exec>
            </tasks>
          </job>
        </jobs>
      </stage>
    </pipeline>
  </pipelines>